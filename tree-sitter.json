{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "zscript",
      "camelcase": "ZScript",
      "title": "ZScript",
      "scope": "source.zs",
      "file-types": [
        "zs",
        "zsc"
      ],
      "injection-regex": "^zscript$",
      "class-name": "TreeSitterZscript"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "ZScript grammar for Tree-sitter",
    "authors": [
      {
        "name": "Jeffrey Crochet",
        "email": "jlcrochet91@pm.me",
        "url": "https://github.com/jlcrochet"
      }
    ],
    "links": {
      "repository": "https://github.com/jlcrochet/tree-sitter-zscript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}